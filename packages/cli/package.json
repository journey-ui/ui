{
  "name": "@journey-ui/cli",
  "version": "0.0.10",
  "description": "CLI for adding Journey UI components to your project",
  "type": "commonjs",
  "bin": {
    "journey-ui": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format cjs --target node18 --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ui",
    "components",
    "cli",
    "react",
    "tailwind"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@antfu/ni": "^25.0.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cosmiconfig": "^8.3.6",
    "execa": "^7.2.0",
    "fs-extra": "^11.1.1",
    "kleur": "^4.1.5",
    "ora": "^6.3.1",
    "prompts": "^2.4.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.41.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.8",
    "tsup": "^7.2.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
